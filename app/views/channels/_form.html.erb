<div class="form-wrap clearfix">
  <h1>Новый район</h1>
  <hr>
  <%= flash[:notice]%>
<%= simple_form_for @channel, :html => { :class => 'form-horizontal' } do |f| %>

    <%= f.input :name, label: "название" %>
    <div id="submit">
      <%= f.button :submit, :value => "сохранить"%>
      <div>
        <%= link_to 'к списку', channels_path, class: "btn"%>
      </div>
    </div>
<% end %>
</div>