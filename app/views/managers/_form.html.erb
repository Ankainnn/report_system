<%= simple_form_for(@manager) do |f| %>
    <%= f.input :name , label: "имя"%>
    <%= f.input :phone, label: "телефон" %>
    <%= f.input :idvk , label: "id-vk"%>
    <%= f.input :school, label: "школа" %>
    <div id="submit">
      <%= f.button :submit, :value => "сохранить"%>
      <div>
        <%= link_to 'к списку', managers_path, class: "btn"%>
      </div>
    </div>
<% end %>
