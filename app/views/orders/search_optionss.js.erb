$('#select_list').html("<%=j render partial: 'select_list' %>");

<%if params[:teacher_id].present? %>
$("#teacher_id_ [value='<%= params[:teacher_id]%>']").attr("selected", "selected");

    <%if params[:schedule_id].present? %>
    $("#schedule_id_ [value='<%= params[:schedule_id]%>']").attr("selected", "selected");

        $('#hours').val("<%= @hours%>");

        <%if @start%>
        $('#start').val("<%= @start%>");
        <%end%>

        <%if @end%>
        $('#end').val("<%= @end%>");
        <%end%>

        <%if @office.present? %>
        $('#office_name_').val("<%= @office.name%>");
        $('#office_id_').val("<%= @office.id%>");
        <%end%>

    <%else%>
        $('#order_start').val("");
        $('#order_end').val("");
        $('#hours').val("");
    <%end%>

<%else%>
    $('#order_start').val("");
    $('#order_end').val("");
    $('#order_hours').val("");
<%end%>


